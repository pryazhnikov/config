if [ -d $SHELL_CONFIG_PATH ]
then
    echo Shell config path: $SHELL_CONFIG_PATH
else
    echo Shell config path not found: $SHELL_CONFIG_PATH
    return
fi

source ${SHELL_CONFIG_PATH}include/aliases
source ${SHELL_CONFIG_PATH}include/export

LOCAL_INCLUDE_FILE=${SHELL_CONFIG_PATH}include/local
if [ -f $LOCAL_INCLUDE_FILE ]
then
    source $LOCAL_INCLUDE_FILE
fi
